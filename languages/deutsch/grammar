#other
haben - hatte - gehabt
kommen - kam - gekommen
sein - war - gewesen
stehen - stand - gestanden
tun - tat - getan

#regular -en[dn]en
brennen -> brannte
kennen -> kannte
nennen -> nannte
rennen -> rannte
senden -> sandte
wenden -> wandte
--
bringen -> brachte
denken -> dachte
--
mögen -> mochte
wissen -> wusste

###present (subjunctive)
befehlen -> befiehlt
empfehlen -> empfiehlt
geschehen -> geschieht
lesen -> liest
sehen -> sieht
stehlen -> stiehlt
--
brechen -> bricht
erschrecken -> erschrickt
essen -> isst
fressen -> frisst
geben -> gibt
gelten -> gilt
helfen -> hilft
messen -> misst
*nehmen -> nimmt
schwellen -> schwillt
sprechen -> spricht
stechen -> sticht
sterben -> stirbt
treffen -> trifft
treten -> tritt
verbergen -> verbirg
verderben -> verdirb
vergessen -> vergisst
werben -> wirbt
werden -> wird
werfen -> wirft
stossen -> stösst
--
dürfen -> 1+3 darf
können -> 1+3 kann
mögen -> 1+3 mag (möchte)
müssen -> 1+3 muss
sollen -> 1+3 soll
wissen -> 1+3 weiss (wüsste)
wollen -> 1+3 will
#a->ä all except schaffen

#new words
noun + 'en' => verb
verb + 'er' => r Noun
verb + 'nis' => s Noun
verb + 'ung' => e Noun
verb + 'schaft' => e Noun
verb + 'heit' => e Noun
verb + 'keit' => e Noun
verb + 'end' => adjective
noun + 'los' => adjective
noun + 'voll' => adjective
verb/noun + 'lich' => adjective
verb/noun + 'isch' => adjective
verb/noun + 'ig' => adjective
